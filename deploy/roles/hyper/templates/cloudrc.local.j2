# -*- mode: sh -*-
cloud_no=0
cloud_domain=hypercube.bluecat.net
image_repo=http://{{ hostvars[groups['hyper'][0]]['inventory_hostname'] }}:4000
dns_server=114.114.114.114
external_vlan=5000
internal_vlan=5010
vxlan_mcast_addr=225.10.10.1
vxlan_interface={{ network_device }}
