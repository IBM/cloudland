{{if .Flash.ErrorMsg}}
	<div class="ui negative message">
		<p>{{.Flash.ErrorMsg}}</p>
	</div>
{{end}}
{{if .Flash.WarningMsg}}
	<div class="ui warning message">
		<p>{{.Flash.WarningMsg}}</p>
	</div>
{{end}}
{{if .Flash.SuccessMsg}}
	<div class="ui positive message">
		<p>{{.Flash.SuccessMsg}}</p>
	</div>
{{end}}
{{if .Flash.InfoMsg}}
	<div class="ui info message">
		<p>{{.Flash.InfoMsg}}</p>
	</div>
{{end}}
